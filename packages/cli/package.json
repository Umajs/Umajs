{
  "name": "@umajs/cli",
  "version": "2.0.4",
  "description": "uma cli",
  "author": "",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "bin": {
    "uma": "./bin/index.js",
    "umajs": "./bin/index.js"
  },
  "files": [
    "lib",
    "bin",
    "index.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "fix": "esw src --fix --ext .ts",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src --ext .ts",
    "lint-w": "ESLINT_USE_FLAT_CONFIG=false esw src --clear --color -w --ext .ts",
    "build-w": "tsc -w --inlineSourceMap",
    "start": "run-p lint-w build-w",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "commander": "^9.0.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.6",
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-watch": "^8.0.0",
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wuba/Umajs.git"
  },
  "bugs": {
    "url": "https://github.com/wuba/Umajs/issues"
  },
  "homepage": "https://github.com/wuba/Umajs#readme",
  "gitHead": "09332985b53ab8ba55636751fa2ec70990c39aa6"
}
